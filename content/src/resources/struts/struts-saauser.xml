<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="saaUser" extends="struts-default" namespace="/saaUser">
		<action name="queryUser" class="saaUserAction" method="queryUser">
			<result name="success">/pages/hy369/systemManager/userManager/UserList.jsp</result>
		</action>
		<action name="prepareQueryUser" class="saaUserAction" method="prepareQueryUser">
			<result name="success">/pages/hy369/systemManager/userManager/UserList.jsp</result>
		</action>
		<action name="prepareAddUser" class="saaUserAction" method="prepareAddUser">
			<result name="success">/pages/hy369/systemManager/userManager/UserEdit.jsp</result>
		</action>
		<action name="addUser" class="saaUserAction" method="addUser">
			<result name="success">/pages/common/addSuccess.jsp</result>
		</action>
		<action name="prepareUpdateUser" class="saaUserAction" method="prepareUpdateUser">
			<result name="success">/pages/hy369/systemManager/userManager/UserEdit.jsp</result>
		</action>
		<action name="updateUser" class="saaUserAction" method="updateUser">
			<result name="success">/pages/common/addSuccess.jsp</result>
		</action>
		<action name="deleteUser" class="saaUserAction" method="deleteUser">
			<result name="success">/pages/hy369/systemManager/userManager/UserList.jsp</result>
		</action>
	</package>
</struts>
