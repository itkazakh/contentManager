<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<bean id="imageService" class="ins.framework.image.ImageService">
		<property name="sourceDir">
			<value>${image.sourceDir}</value>
		</property>
		<property name="destinationDir">
			<value>${image.sourceDir}</value>
		</property>
		<property name="width">
			<value>${image.width}</value>
		</property>
		<property name="height">
			<value>${image.height}</value>
		</property>
		<property name="mode">
			<value>${image.mode}</value>
		</property>
		<property name="characterStorage">
			<value>${image.characterStorage}</value>
		</property>
	</bean>
</beans>
